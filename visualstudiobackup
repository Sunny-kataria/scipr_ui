    <script src="/Common/Common.js"></script>

    <script type="text/JavaScript">
        $(document).ready(function () {            

            var f = $("#hfPosition");
            window.onload = function () {
                var position = parseInt(f.val());
                if (!isNaN(position)) {
                    $(window).scrollTop(position);
                }
            };
            window.onscroll = function () {
                var position = $(window).scrollTop();
                f.val(position);
            };
        });

        var globaltime;
        var tik;
        var popup;
        var existsite;

        function SessionExpireAlert(timeout) {
            window.clearTimeout(popup);
            globaltime = timeout;
            var seconds = globaltime / 1000;
            $('#secondsIdle').html(seconds);
            tik = setInterval(function () {
                seconds--;
                $('#secondsIdle').text(seconds);
                $('#aaa').html(seconds);
            }, 1000);

            popup = setTimeout(function () { show(); }, timeout - 60 * 1000);
            existsite = setTimeout(function () {
                window.location = "../../Sessionxp.aspx";
            }, timeout);
        }

        function ResetSession() {
            HideModalPopUp();
            $.ajax({
                type: "POST",
                url: "/Common/SessionTimeOutTB.aspx/GetTranscationHistory",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: OnSuccess
            });
        }
        function OnSuccess() {
            clearInterval(tik);
            window.clearTimeout(popup);
            window.clearTimeout(existsite);
            setInterval(SessionExpireAlert(globaltime), 10);
        }

        function show() {
            ResizePopUp();
            $('#sessionExpire').show();
            $('#sessionExpire_overlay').show();
            window.scroll(0, 0);
            $('#focus').attr("tabindex", -1).focus();
        }
        function HideModalPopUp() {
            var divFrameParent = document.getElementById("sessionExpire");
            var divOverlay = document.getElementById("sessionExpire_overlay");
            divOverlay.style.display = "none";
            divFrameParent.style.display = "none";
        }

        function ResizePopUp() {
            var divFrameParent = document.getElementById("sessionExpire");
            var divOverlay = document.getElementById("sessionExpire_overlay");

            var winW = Math.round(($(window).width()) / 2);
            var popW = Math.round(($('#sessionExpire').width()) / 2);
            var winH = Math.round(($(window).height()) / 2);
            var popH = ($('#sessionExpire').height()) / 2;
            var top = Math.round((winH - popH));
            var left = Math.round((winW - popW));
            divFrameParent.style.top = top + "px";
            divFrameParent.style.left = left + "px";
            divOverlay.style.top = "0px";
            divOverlay.style.left = "0px";
            var e = document;
            var c = "Height";
            var maxHeight = Math.max(e.documentElement["client" + c], e.body["scroll" + c], e.documentElement["scroll" + c], e.body["offset" + c], e.documentElement["offset" + c]);
            c = "Width";
            var maxWidth = Math.max(e.documentElement["client" + c], e.body["scroll" + c], e.documentElement["scroll" + c], e.body["offset" + c], e.documentElement["offset" + c]);
            divOverlay.style.height = maxHeight + "px";
            divOverlay.style.width = maxWidth - 2 + "px";
        }
        function findPos(obj) {
            var curtop = 0;
            if (obj.offsetParent) {
                do {
                    curtop += obj.offsetTop;
                } while (obj = obj.offsetParent);
                return [curtop];
            }
        }

        function MM_openBrWindow(theURL, winName, features) {
            OpenPopup(theURL, winName, features);
        }

        function Open_popup() {
            var name = "http://canadianjournalofpathology.ca/";
            var ht;
            if (window.navigator.appVersion.indexOf("MSIE 5.01") == -1)
                ht = 300;
            else
                ht = 600;
            OPenlink = window.open(name, "special_characters", "resizable=yes,scrollbars=yes,fullscreen=yes,dependent=yes,alwaysRaised=yes");
            OPenlink.focus();
        }

        function Open_popup_cover() {
            var name = "http://canadianjournalofpathology.ca/";
            var ht;
            if (window.navigator.appVersion.indexOf("MSIE 5.01") == -1)
                ht = 300;
            else
                ht = 600;
            OPenlink = window.open(name, "special_characters", "resizable=yes,scrollbars=yes,fullscreen=yes,dependent=yes,alwaysRaised=yes");
            OPenlink.focus();
        }
        function Open_popup_det() {
            var name = "http://sac-oac.ca/";
            var ht;
            if (window.navigator.appVersion.indexOf("MSIE 5.01") == -1)
                ht = 300;
            else
                ht = 600;
            OPenlink = window.open(name, "special_characters", "resizable=yes,scrollbars=yes,fullscreen=yes,dependent=yes,alwaysRaised=yes");
            OPenlink.focus();
        }        
</script>   
    
</head>
